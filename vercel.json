{
  "functions": {
    "pages/api/analyze.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    
     "pages/api/generate-search-strings.js": {
      "maxDuration": 60,
      "memory": 1024,
      "runtime": "nodejs18.x"
    },

    "pages/api/search-mesh.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, max-age=0"
        }
      ]
    }
  ]
}